<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication4</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css">
    <link href="dashboard.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication4.styles.css" asp-append-version="true" />
</head>

<body>
    < <div class="container-fluid">
        <div class="row">
           

           <nav class="  bg-light sidebar d-flex" style="width:270px">
                <div class="sub-nav text-center d-flex flex-column p-3">
                    <h1 class="mt-2 text-white">IN</h1>
                    <button class="btn my-3 p-2" style="background-color: rgba(240, 248, 255, 0.103);color: white;"> <span data-feather="file"></span></button>
                    <button class="btn my-3 p-2" style="background-color: rgba(240, 248, 255, 0.103);color: white;"> <span data-feather="file"></span></button>
                    <button class="btn my-3 p-2" style="background-color: rgba(240, 248, 255, 0.103);color: white;"> <span data-feather="file"></span></button>
                    <button class="btn my-3 p-2" style="background-color: rgba(240, 248, 255, 0.103);color: white;"> <span data-feather="file"></span></button>
                    <button class="btn my-3 p-2" style="background-color: rgba(240, 248, 255, 0.103);color: white;"> <span data-feather="file"></span></button>
                </div>
                
                <div class="sidebar-sticky w-100">
                    <div class="d-flex mt-2">
                        <button class="btn py-2 px-3 " style="background-color: rgba(240, 248, 255, 0.103);border-radius: 1000px;color: white;margin-left: 15px;"> <span data-feather="user"></span></button>
                        <h5 class="ml-2 text-white mt-1"> username</h5>
                    </div>
                    <ul class="nav flex-column">

                        <li class="nav-item">
                            <h6
                                class=" sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                                <span>Dashboard</span>

                            </h6>
                        </li>
                        <li class="nav-item  ">
                            <a class="nav-link text-light  text-light " asp-controller="Home" asp-action="Index">
                                <span data-feather="file"></span> Trang chủ
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-controller="CreateOrder" asp-action="Index">
                                <span data-feather="shopping-cart"></span> Tạo đơn hàng
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-controller="TableOrder" asp-action="Index">
                                <span data-feather="users"></span> Bảng đơn hàng
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-controller="UserPartner" asp-action="Index">
                                <span data-feather="bar-chart-2"></span> Thành viên
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-controller="GroupPartner" asp-action="Index">
                                <span data-feather="layers"></span> Nhóm thành viên
                            </a>
                        </li>
                    </ul>

                    <h6
                        class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                        <span>Hỗ trợ</span>

                    </h6>
                    <ul class="nav flex-column mb-2">
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-controller="ChangeInfo" asp-action="Index">
                                <span data-feather="file-text"></span> Thay đổi thông tin
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " href="#">
                                <span data-feather="file-text"></span> Last quarter
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " href="#">
                                <span data-feather="file-text"></span> Social engagement
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " href="#">
                                <span data-feather="file-text"></span> Year-end sale
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>

            <main role="main" class="" style="width:100%;margin-left:260px">
                @RenderBody()
            </main>
        </div>
        </div>












        <!-- Placed at the end of the document so the pages load faster -->
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"></script>

        <!-- Icons -->
        <script src="https://unpkg.com/feather-icons/dist/feather.min.js"></script>
        <script>
            feather.replace()
        </script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
        @RenderSection("reactScript", required: false)
</body>

</html>
